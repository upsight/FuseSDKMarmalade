#!/usr/bin/env mkb
# Builder mkb file for the FuseAPI extension on iOS
platform IPHONE

options
{
    lib
    output-name="lib/iphone/libFuseAPIWrapper.a"
    output-name-debug="lib/iphone/libFuseAPIWrapper_d.a"
}
define S3E_BUILD_EXT

subproject FuseAPI_build

#run a post build step to fix return statements in FuseAPI_interface.cpp after it is generated/modified
command
{
	"python ./tools/fix_interface.py"
}